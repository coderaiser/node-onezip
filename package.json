{
  "name": "onezip",
  "version": "2.0.2",
  "description": "pack and extract .zip archives with emitter",
  "main": "lib/onezip.js",
  "bin": {
    "onezip": "bin/onezip.js"
  },
  "scripts": {
    "lint:eslint": "eslint lib",
    "lint:eslint:bin": "eslint bin --config .eslint-bin.rc",
    "lint": "redrun lint:*",
    "test": "tape 'test/**/*.js'",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "watcher": "nodemon -w test -w lib --exec",
    "watch:test": "npm run watcher -- npm test",
    "watch:coverage": "npm run watcher -- npm run coverage"
  },
  "dependencies": {
    "findit2": "^2.2.3",
    "glob": "^7.0.0",
    "itchy": "^1.0.0",
    "mkdirp": "^0.5.1",
    "pipe-io": "^3.0.0",
    "yargs-parser": "^10.0.0",
    "yauzl": "^2.6.0",
    "yazl": "^2.4.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^5.6.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.2",
    "redrun": "^6.0.0",
    "rimraf": "^2.5.4",
    "tape": "^4.6.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coderaiser/node-onezip"
  },
  "keywords": [
    "extract",
    "zip",
    "unzip",
    "pack"
  ],
  "author": "coderaiser <mnemonic.enemy@gmail.com> (http://coderaiser.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coderaiser/node-onezip/issues"
  },
  "homepage": "https://github.com/coderaiser/node-onezip"
}
